<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:fo="http://www.w3.org/1999/XSL/Format" xmlns:msxsl="urn:schemas-microsoft-com:xslt" exclude-result-prefixes="msxsl">
  <xsl:variable name="itemCount">
    <xsl:value-of select="count(Invoice//Detail//InvoiceDetail)" />
  </xsl:variable>
  <xsl:output omit-xml-declaration="yes" indent="yes" encoding="UTF-8" />
  <xsl:template match="/*">
    <html>
      <head>
	    <meta http-equiv="Content-Type" content="text/html;charset=utf-8"/>
        <title>Hóa đơn bán hàng</title>
		<script src="http://html2canvas.hertzen.com/dist/html2canvas.min.js">(function(){})()</script>
        <style type="text/css">			
			@page { size: auto;	margin: 2mm 2mm 2mm 2mm; } 
			#invoice { padding: 0; zoom: 50%; font-family: sans-serif;}
			.table-invoice { border-collapse: collapse; width: 100%}
			.table-invoice tr th,table tr td { padding: 3px; border: 0px solid black; }
			.table-invoice tr { text-align: center;}
	        #FoodName, #Unit { text-align: left }
			#Price, #Amount { text-align: right }
			.text-center { text-align: center; }
			.text-left { text-align: left; }
			.text-right { text-align: right; }
			.container { width: 500px; margin: 0 auto; padding: 20px;}
			.dashed-bottom { border-bottom: 1px dashed black; padding-bottom: 10px; }
			#info { margin-bottom: 10px; }
			#info table tr td { border: none; text-align: left;}
			.bold { font-weight: bold; }
			#InvoiceName { margin-bottom: 7px }
			.tb-small { width: 11% }
			.tb-big { width: 40% }
		    #last-row td, #payment tr td { border: none; }
			#payment tr td { font-size: 1.5em; }
			#payment { margin-top: 10px; }
			#logo { background: url('data:image/jpeg;base64,');
			background-size: cover; width: 50%; margin: 0 auto; }
			#header-logo { width: 60%; margin: 0 auto; }
			#footer_4 { margin-top: 10px; font-style: italic }
			#InvoiceName { font-size: 1.5em; font-family: Arial; margin-top: 0;}
			.pt-15 { padding-top: 15px;}
			.pt-5 { padding-top: 5px;}
			#footer_5 { font-size: 10px; color: blue; }
			.money { font-size: 1.5em; }
        </style>
      </head>
      <body>	
        <div class="container" id="invoice">
          <div id="header" class="text-center">
            <div id="logo">
              <img width="100%" src="data:image/jpeg;base64,"></img>
            </div>
            <h1 class="text-center" id="Name">
              <xsl:value-of select="Restaurant/Name"/>
            </h1>
            <p class="text-center">
              <xsl:value-of select="Restaurant/Description"/>
            </p>
          </div>
          <div id="content">
            <h3 class="text-center dashed-bottom" id="InvoiceName">
              <xsl:value-of select="InvoiceName"/>
            </h3>
            <div id="info" class="dashed-bottom">
              <table>
                <tr>
                  <td class="tb-small">✎ <b>Số HĐ</b></td>
                  <td class="tb-big">: <xsl:value-of select="InvoiceCode"/></td>

                </tr>
                <tr>
                  <td class="tb-small">✎ <b>Bàn</b></td>
                  <td class="tb-big">: <xsl:value-of select="DinnerTable"/></td>                
                </tr>
                <tr>
                  <td class="tb-small">✎ <b>Thu ngân</b></td>
                  <td class="tb-big">: <xsl:value-of select="UserCreate"/></td>
                </tr>
                <tr>
                  <td class="tb-small">✎ <b>Ngày</b></td>
                  <td class="tb-big">: <xsl:value-of select="CreateDate"/></td>
                </tr>
              </table>
            </div>
            <div class="dashed-bottom">
              <table class="table-invoice">
                <tr class="dashed-bottom">
                  <th class="text-left">Tên món ăn</th>
                  <th>Số lượng</th>
                  <th class="text-right">Đơn giá</th>
                  <th class="text-right">Thành tiền</th>
                </tr>
                <xsl:for-each select="Detail/InvoiceDetail">
                  <tr>
                    <td id="FoodName">
					  <xsl:variable name="i" select="position()" />
                      <xsl:value-of select="$i"/>
                      .<xsl:value-of select="FoodName"/>
                    </td>
                    <td id="Quantity">
                      <xsl:value-of select="Quantity"/>
                    </td>
                    <td id="Price">
                      <xsl:value-of select="translate(translate(translate(format-number(translate(Price,',','.'), '###,##0.##'),',','?'),'.',','),'?','.')"/>
                    </td>
                    <td id="Amount">
                      <xsl:value-of select="translate(translate(translate(format-number(translate(Amount,',','.'), '###,##0.##'),',','?'),'.',','),'?','.')"/>
                    </td>
                  </tr>
                </xsl:for-each>
                <tr id="last-row">
                  <td class="text-left bold pt-15" colspan="3">Thành Tiền:</td>
                  <td class="text-right bold money pt-15"><xsl:value-of select="translate(translate(translate(format-number(translate(SumAmount,',','.'), '###,##0.##'),',','?'),'.',','),'?','.')"/> đ</td>
                </tr>
				<tr id="last-row">
                  <td class="text-left bold" colspan="3">Giảm trừ khuyến mại:</td>
                  <td class="text-right money bold"><xsl:value-of select="translate(translate(translate(format-number(translate(ReduceAmount,',','.'), '###,##0.##'),',','?'),'.',','),'?','.')"/>  đ</td>
                </tr>
              </table>
            </div>
            <div id="payment" class="dashed-bottom">
              <table class="table-invoice">
                <tr>
                  <td class="bold text-left">Tiền Thanh Toán:</td>
                  <td class="bold money text-right"><xsl:value-of select="translate(translate(translate(format-number(translate(Payment,',','.'), '###,##0.##'),',','?'),'.',','),'?','.')"/> đ</td>
                </tr>
                <tr>
                  <td class="bold text-left">Tiền Khách Đưa:</td>
                  <td class="bold money text-right"><xsl:value-of select="translate(translate(translate(format-number(translate(GuestPut,',','.'), '###,##0.##'),',','?'),'.',','),'?','.')"/> đ</td> 
                </tr>
                <tr>
                  <td class="bold text-left">Tiền Trả Khách:</td>
                  <td class="bold money text-right"><xsl:value-of select="translate(translate(translate(format-number(translate(GuestReceive,',','.'), '###,##0.##'),',','?'),'.',','),'?','.')"/> đ</td>
                </tr>
              </table>
            </div>			
          </div>
		  <div id="footer">				
				<div id="footer_4" class="text-center dashed-bottom">
					Chúc quý khách ngon miệng
				</div>
        <div id="footer_5" class="text-center pt-5">
          VNPT - eRestaurant - Hệ thống quản lý nhà hàng
        </div>
			</div>
        </div>	
      </body>
    </html>
  </xsl:template>
</xsl:stylesheet>
